OBJ_GRID2D = $(OBJDIR)/grid2D.o $(OBJDIR)/csProfile.o $(OBJDIR)/csLine3D.o
MAIN_GRID2D = $(BINDIR)/grid2D.exe
#OBJ_HELP = $(OBJDIR)/cseis_help.o $(OBJDIR)/csHelp.o

SEGDDIR   = src/cs/segd/

MAIN = $(BINDIR)/seaseis.exe
#HELP = $(BINDIR)/cseis_help.exe

CXXFLAGS_MAIN = $(COMMON_FLAGS) -I"src/cs/geolib" -I"src/cs/system" -I"src/cs/segy"
CXXFLAGS_MODULES = -I"src/cs/geolib"  -I"src/cs/system" -I"src/cs/io" -I"src/cs/segy"  -I"src/cs/segd" $(COMMON_FLAGS)
CXXFLAGS_GEOLIB = $(COMMON_FLAGS) -I"src/cs/geolib"
CXXFLAGS_SEGY   = $(COMMON_FLAGS) -I"src/cs/geolib" -I"src/cs/segy"
CXXFLAGS_SEGD   = $(COMMON_FLAGS) -I"src/cs/geolib" -I"src/cs/modules/input_segd"
CXXFLAGS_SYSTEM = $(COMMON_FLAGS) -I"src/cs/geolib" -I"src/cs/io" -I"src/cs/system"


default:  $(OBJ_SYSTEM) $(OBJ_METHODS) $(OBJ_IO) $(OBJ_GEOLIB) $(OBJ_SEGY) $(OBJ_SEGD) $(OBJ_MODULES) $(OBJ_MAIN) $(OBJ_GRID2D) $(MAIN) $(MAIN_GRID2D)

clean:
	${RM} $(OBJ_SYSTEM) $(OBJ_METHODS) $(OBJ_IO) $(OBJ_GEOLIB) $(OBJ_SEGY) $(OBJ_SEGD) $(OBJ_MODULES) $(OBJ_RAY2D) $(OBJ_MAIN) $(OBJ_GRID2D)

bleach: clean
	${RM} $(MAIN) $(MAIN_GRID2D)


$(MAIN): $(OBJ_MAIN)
	$(CPP) $(GLOBAL_FLAGS) -static-libgcc -static-libstdc++ -static-libgfortran -static $(OBJ_MAIN) $(OBJ_SYSTEM) $(OBJ_IO) $(OBJ_GEOLIB) $(OBJ_SEGY) $(OBJ_SEGD) $(OBJ_MODULES) $(OBJ_RAY2D) $(OBJ_GRID2D) -o $(MAIN) -lgfortran

